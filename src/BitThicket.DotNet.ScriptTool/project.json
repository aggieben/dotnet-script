{
  "version": "0.1.0-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "outputName": "dotnet-script",
    "preserveCompilationContext": true
  },
  "packOptions": {
      "repository": {
          "type": "git",
          "url": "https://github.com/aggieben/dotnet-script"
      },
      "licenseUrl": "https://github.com/aggieben/dotnet-script/blob/master/LICENSE.md",
      "owners": [
          "Ben Collins (@aggieben)"
      ],
      "tags": [
          "dotnet",
          ".NET Core"
      ]
  },
  "dependencies": {
      "Microsoft.CodeAnalysis.CSharp.Scripting": "1.3.2",
      "Microsoft.Extensions.DependencyModel": "1.0.0",
      "System.Diagnostics.TraceSource": "4.0.0",
      "System.Runtime.Loader": "4.0.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": [          
          "dnxcore50",
          "dotnet"
      ]
    }
  }
}
