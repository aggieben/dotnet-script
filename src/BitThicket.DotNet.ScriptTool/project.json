{
  "version": "0.1.1-*",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "outputName": "dotnet-script",
    "preserveCompilationContext": true
  },
  "packOptions": {
      "repository": {
          "type": "git",
          "url": "https://github.com/aggieben/dotnet-script"
      },
      "licenseUrl": "https://github.com/aggieben/dotnet-script/blob/master/LICENSE.md",
      "owners": [
          "Ben Collins (@aggieben)"
      ],
      "tags": [
          "dotnet",
          ".NET Core"
      ]
  },
  "dependencies": {
      "System.CommandLine": {
          "target": "project"
      },
      "Microsoft.CodeAnalysis.CSharp.Scripting": "2.0.0-rc2",
      "Microsoft.CodeAnalysis.Scripting": "2.0.0-rc2",
      "Microsoft.Extensions.Logging": "1.1.0",
      "Microsoft.Extensions.Logging.Console": "1.1.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
        "dependencies": {
            "Microsoft.NETCore.App": {
                "type": "platform",
                "version": "1.1.0"
            }
        }
    }
  }
}
